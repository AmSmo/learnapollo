{
  "name": "dog-toy-demo",
  "version": "1.0.0",
  "description": "Dog Toy Demo for Apollo",
  "main": "index.js",
  "scripts": {
    "watch": "tsc -w",
    "start": "nodemon dist/index.js",
    "startJS": "node dist/index.js",
    "server": "nodemon dist/index.js",
    "frontend": "yarn dev --prefix graphdog",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "create:migration": "mikro-orm migration:create"
  },
  "author": "Rose Koron",
  "dependencies": {
    "@graphql-codegen/cli": "^1.20.1",
    "@mikro-orm/cli": "^4.4.2",
    "@mikro-orm/core": "^4.4.2",
    "@mikro-orm/migrations": "^4.4.2",
    "@mikro-orm/postgresql": "^4.4.2",
    "@types/react-router-dom": "^5.1.7",
    "apollo-server-express": "^2.19.2",
    "argon2": "^0.27.1",
    "body-parser": "^1.18.3",
    "concurrently": "^5.3.0",
    "connect-redis": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.7.1",
    "express-session": "^1.17.1",
    "graphql": "^15.5.0",
    "nodemon": "^1.18.10",
    "pg": "^8.5.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.1",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.16",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/node": "^14.14.25",
    "@types/redis": "^2.8.28",
    "typescript": "^4.1.3"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
