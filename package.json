{
  "name": "dog-toy-demo",
  "version": "1.0.0",
  "description": "Dog Toy Demo for Apollo",
  "main": "index.js",
  "scripts": {
    "watch": "tsc -w",
    "start": "ts-node src/index.ts",
    "startJS": "node dist/index.js",
    "server": "nodemon dist/index.js",
    "frontend": "npm start --prefix graphdog",
    "dev": "concurrently \"npm run server\" \"npm run frontend\""
  },
  "author": "Rose Koron",
  "dependencies": {
    "@graphql-codegen/cli": "^1.20.1",
    "@mikro-orm/cli": "^4.4.2",
    "@mikro-orm/core": "^4.4.2",
    "@mikro-orm/migrations": "^4.4.2",
    "@mikro-orm/postgresql": "^4.4.2",
    "@types/react-router-dom": "^5.1.7",
    "apollo-server-express": "^2.19.2",
    "body-parser": "^1.18.3",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "express-graphql": "^0.7.1",
    "graphql": "^15.5.0",
    "mongoose": "^5.4.13",
    "nodemon": "^1.18.10",
    "pg": "^8.5.1",
    "ts-node": "^9.1.1",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.25",
    "typescript": "^4.1.3"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
